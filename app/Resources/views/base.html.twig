<!doctype html>
<html>
<head>
    <!-- Global site tag (gtag.js) - AdWords: 867848012 -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=AW-867848012"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'AW-867848012');
    </script>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-66526021-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-66526021-1');
    </script>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    {% block metas %}
        <meta property="robots" content="index,follow"/>
        <meta property="googlebot" content="index, follow"/>
        {#<meta property="revisit-after" content="2 days"/>#}
        <meta property="language" content="pt-br"/>
        <meta property="rating" content="general"/>
        <meta property="page-topic"
              content="{{ metaPageTopic is defined ? metaPageTopic : 'Rogério Menezes Leiloeiro Oficial' }}"/>
        <meta property="classification"
              content="{{ metaPageClassification is defined ? metaPageClassification : 'Rogério Menezes Leiloeiro Oficial' }}"/>
        <meta property="distribution" content="Global"/>
        <meta property="author" content="Wtis"/>
        <meta property="copyright" content="wtis"/>
        <meta property="og:type" content="website">
        <meta name="theme-color" content="#000000" />
        {#<meta http-equiv="refresh" content="5">#}
    {% endblock %}
    {% block title %}
        <title>Rogério Menezes - Leiloeiro Oficial</title>
    {% endblock %}
    {% block stylesheets %}
        {% if app.environment == 'dev' %}
            <link rel="stylesheet" type="text/css" href="{{ asset('css/jquery-ui.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('css/bootstrap.min.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('css/tipTip.css') }}">
            <link rel="stylesheet" type="text/css" href="{{ asset('css/style.css') }}"/>
            <link rel="stylesheet" type="text/css" href="{{ asset('css/font-awesome.min.css') }}"/>
        {% else %}
            <link href="{{ asset('css/app.min.css') }}" rel="stylesheet">
        {% endif %}
    {% endblock %}
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->


</head>

<body>
<!-- Modals -->
<!-- Incluir Aqui -->

<!-- HEADER -->
{% include "header.html.twig" %}
<div class="clearfloat"></div>
<!--/ HEADER -->
<!-- CONTENT -->
<div class="content-overflow">
    {% block body %}{% endblock %}
</div>
<!--//// CONTENT -->
<!-- FOOTER -->
{% include "footer.html.twig" %}
<!--//// FOOTER -->

<!-- History Bar -->
{% include "history-bar/historybar.html.twig" %}

{% block javascripts %}
    <script type="text/javascript">
      var services = {};
      services.FavoritosListar = {
        lote: "{{ path('uapi_history_favoritos_lotes') }}",
        leilao: "{{ path('uapi_history_favoritos_leiloes') }}"
      };
      services.FavoritosUrl = {
        lote: "{{ path('uapi_history_favoritos_add_lote', {id: '__ID__'}) }}",
        leilao: "{{ path('uapi_history_favoritos_add_leilao', {id: '__ID__'}) }}"
      };
      {% if is_granted('ROLE_USER') %}
      var __USERID__ = {{ app.user.id }};
      {% else %}
      var __USERID__ = null;
      {% endif %}
      {% if app.environment == 'dev' %}
      var __WSSERVER__ = "{{ ws }}";
      {% else %}
      var __WSSERVER__ = "{{ wss }}";
      {% endif %}
    </script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/php-js/number_format.js') }}"></script>
    {% if app.environment == 'dev' %}
    <script type="javascript">var env = 'dev';</script>
    <script src="{{ asset('js/vendor/jquery-ui.min.js') }}"></script>
    <script src="{{ asset('js/vendor/bootstrap.min.js') }}"></script>
    {#<script type="text/javascript" src="{{ asset('js/vendor/jquery.parallax-1.1.3.js') }}"></script>#}
    <script type="text/javascript" src="{{ asset('js/vendor/jquery.localscroll-1.2.7-min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/jquery.scrollTo-1.4.2-min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/jquery.tipTip.minified.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/scrollReveal.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/vendor/jquery.mask.min.js') }}"></script>
    {#<script type="text/javascript" src="{{ asset('js/vendor/jquery.nicescroll.min.js') }}"></script>#}
    {#<script type="text/javascript" src="{{ asset('js/vendor/parsley.min.js') }}"></script>#}
    <script type="text/javascript" src="{{ asset('js/vendor/vue.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/script.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/historyBar.js') }}"></script>
    {% else %}
    <script type="javascript">var env = 'prod';</script>
    <script src="{{ asset('js/app.min.js') }}"></script>
    {% endif %}
{% endblock %}
</body>
</html>
